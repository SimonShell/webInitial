        "daily-build": "npm run cleanAndInstall && cross-env NODE_ENV=daily webpack --progress -p --config ./webpack/daily.js",
        "pre-build": "npm run cleanAndInstall &&  cross-env NODE_ENV=prevProduction webpack --progress -p --config ./webpack/prevProduction.js",
        "dist-build": "npm run cleanAndInstall && cross-env NODE_ENV=production webpack -p --config ./webpack/build.js",
